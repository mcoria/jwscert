<?xml version="1.0" encoding="utf-8"?>
<bindings xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns="http://java.sun.com/xml/ns/jaxws"
	wsdlLocation="../wsdl/globalweather.wsdl">
	
	<package name="net.wheater.ws"/>
	
	<!-- 
    <bindings node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://www.webserviceX.NET']">
        <jaxb:schemaBindings>
            <jaxb:package name="net.wheater.types" />
        </jaxb:schemaBindings>
    </bindings>
     -->	
	
	<bindings node="wsdl:definitions/wsdl:portType[@name='GlobalWeatherSoap']">
		<!-- change the generated SEI class -->
		<class name="GlobalWeatherSEI" />
	</bindings>	

	<bindings node="wsdl:definitions/wsdl:service[@name='GlobalWeather']">
		<!--  change the generated service class  -->
		<class name="GlobalWeatherService"/>
	</bindings>
	
	<bindings node="wsdl:definitions/wsdl:service[@name='GlobalWeather']/wsdl:port[@name='GlobalWeatherSoap']">
		<method name="getGlobalWeatherSEI" />
	</bindings>	
	
</bindings>